        <miglayout.version>3.7.4</miglayout.version>
        <l2fprod.version>1.0.5</l2fprod.version>
        <jcalendar.version>1.4</jcalendar.version>
        <swinglayout.version>1.0.3</swinglayout.version>
        <java-desktop-util.version>${jd2.version}</java-desktop-util.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>eu.europa.ec.joinup.sat</groupId>
                <artifactId>demetra-utils</artifactId>
                <version>${jd2.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            
            <dependency>
                <groupId>eu.europa.ec.joinup.sat</groupId>
                <artifactId>nbdemetra-utils</artifactId>
