            </dependency>
            <dependency>
                <groupId>org.tros</groupId>
                <artifactId>l2fprod-properties-editor</artifactId>
                <version>${l2fprod.version}</version>
            </dependency>
            <dependency>
                <groupId>com.toedter</groupId>
